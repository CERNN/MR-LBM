    break;



















case SOUTH_WEST:
    l1 = -1;
    l2 = -1;
    l3 = 0;
    // u1 = ux // u2  = uy // u3 == uz
    ux1 = 0.0;
    ux2 = 0.0;
    ux3 = 0.0;

    rho_I = pop[0] + pop[2] + pop[4] + pop[5] + pop[6] + pop[8] + pop[10] + pop[12] + pop[16] + pop[18];
    inv_rho_I = 1.0 / rho_I;
    pixx_I = inv_rho_I * (-(1.0/3.0)*pop[0] + (2.0/3.0)*pop[2] - (1.0/3.0)*pop[4] - (1.0/3.0)*pop[5] - (1.0/3.0)*pop[6] + (2.0/3.0)*pop[8] + (2.0/3.0)*pop[10] - (1.0/3.0)*pop[12] + (2.0/3.0)*pop[16] - (1.0/3.0)*pop[18]);
    pixy_I = inv_rho_I * (pop[8]);
    pixz_I = inv_rho_I * (pop[10] - pop[16]);
    piyy_I = inv_rho_I * (-(1.0/3.0)*pop[0] - (1.0/3.0)*pop[2] + (2.0/3.0)*pop[4] - (1.0/3.0)*pop[5] - (1.0/3.0)*pop[6] + (2.0/3.0)*pop[8] - (1.0/3.0)*pop[10] + (2.0/3.0)*pop[12] - (1.0/3.0)*pop[16] + (2.0/3.0)*pop[18]);
    piyz_I = inv_rho_I * (pop[12] - pop[18]);
    pizz_I = inv_rho_I * (-(1.0/3.0)*pop[0] - (1.0/3.0)*pop[2] - (1.0/3.0)*pop[4] + (2.0/3.0)*pop[5] + (2.0/3.0)*pop[6] - (1.0/3.0)*pop[8] + (2.0/3.0)*pop[10] + (2.0/3.0)*pop[12] + (2.0/3.0)*pop[16] + (2.0/3.0)*pop[18]);

    p_x1_x1_I = pixx_I;
    p_x2_x2_I = piyy_I;
    p_x3_x3_I = pizz_I;
    p_x1_x2_I = pixy_I;
    p_x1_x3_I = pixz_I;
    p_x2_x3_I = piyz_I;

    #include "momEdge"

    ux_t30 = ux1;
    uy_t30 = ux2;
    uz_t30 = ux3;

    pixx_t45 = p_x1_x1;
    piyy_t45 = p_x2_x2;
    pizz_t45 = p_x3_x3;
    pixy_t90 = p_x1_x2;
    pixz_t90 = p_x1_x3;
    piyz_t90 = p_x2_x3;

    rhoVar = rho;

    break;
case NORTH_WEST:
    l1 = -1;
    l2 = 1;
    l3 = 0;
    // u1 = ux // u2  = uy // u3 == uz
    ux1 = 0.0;
    ux2 = 0.0;
    ux3 = 0.0;

    rho_I = pop[0] + pop[2] + pop[3] + pop[5] + pop[6] + pop[10] + pop[11] + pop[14] + pop[16] + pop[17];
    inv_rho_I = 1.0 / rho_I;
