    pixx_I = inv_rho_I * (-(1.0/3.0)*pop[0] + (2.0/3.0)*pop[1] - (1.0/3.0)*pop[3] - (1.0/3.0)*pop[6] + (2.0/3.0)*pop[7] + (2.0/3.0)*pop[15] - (1.0/3.0)*pop[17]);
    pixy_I = inv_rho_I * (pop[7]);
    pixz_I = inv_rho_I * (-pop[15]);
    piyy_I = inv_rho_I * (-(1.0/3.0)*pop[0] - (1.0/3.0)*pop[1] + (2.0/3.0)*pop[3] - (1.0/3.0)*pop[6] + (2.0/3.0)*pop[7] - (1.0/3.0)*pop[15] + (2.0/3.0)*pop[17]);
    piyz_I = inv_rho_I * (-pop[17]);
    pizz_I = inv_rho_I * (-(1.0/3.0)*pop[0] - (1.0/3.0)*pop[1] - (1.0/3.0)*pop[3] + (2.0/3.0)*pop[6] - (1.0/3.0)*pop[7] + (2.0/3.0)*pop[15] + (2.0/3.0)*pop[17]);

    p_x1_x1_I = pixx_I;
    p_x2_x2_I = piyy_I;
    p_x3_x3_I = pizz_I;
    p_x1_x2_I = pixy_I;
    p_x1_x3_I = pixz_I;
    p_x2_x3_I = piyz_I;

#include "momCorner"
    ux_t30 = ux1;
    uy_t30 = ux2;
    uz_t30 = ux3;

    pixx_t45 = p_x1_x1;
    piyy_t45 = p_x2_x2;
    pizz_t45 = p_x3_x3;
    pixy_t90 = p_x1_x2;
    pixz_t90 = p_x1_x3;
    piyz_t90 = p_x2_x3;

    rhoVar = rho;

    break;
case NORTH_EAST_FRONT:
    l1 = 1;
    l2 = 1;
    l3 = 1;
    // u1 = ux // u2  = uy // u3 == uz
    ux1 = 0.0;
    ux2 = 0.0;
    ux3 = 0.0;

    rho_I = pop[0] + pop[1] + pop[3] + pop[5] + pop[7] + pop[9] + pop[11];
    inv_rho_I = 1.0 / rho_I;
    pixx_I = inv_rho_I * (-(1.0/3.0)*pop[0] + (2.0/3.0)*pop[1] - (1.0/3.0)*pop[3] - (1.0/3.0)*pop[5] + (2.0/3.0)*pop[7] + (2.0/3.0)*pop[9] - (1.0/3.0)*pop[11]);
    pixy_I = inv_rho_I * (pop[7]);
    pixz_I = inv_rho_I * (pop[9]);
    piyy_I = inv_rho_I * (-(1.0/3.0)*pop[0] - (1.0/3.0)*pop[1] + (2.0/3.0)*pop[3] - (1.0/3.0)*pop[5] + (2.0/3.0)*pop[7] - (1.0/3.0)*pop[9] + (2.0/3.0)*pop[11]);
    piyz_I = inv_rho_I * (pop[11]);
    pizz_I = inv_rho_I * (-(1.0/3.0)*pop[0] - (1.0/3.0)*pop[1] - (1.0/3.0)*pop[3] + (2.0/3.0)*pop[5] - (1.0/3.0)*pop[7] + (2.0/3.0)*pop[9] + (2.0/3.0)*pop[11]);

