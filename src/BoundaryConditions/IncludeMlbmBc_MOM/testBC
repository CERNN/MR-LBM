    ux_t30 = ux2;
    uy_t30 = ux3;
    uz_t30 = ux1;

    pixx_t45 = p_x2_x2;
    piyy_t45 = p_x3_x3;
    pizz_t45 = p_x1_x1;
    pixy_t90 = p_x2_x3;
    pixz_t90 = p_x1_x2;
    piyz_t90 = p_x1_x3;

    rhoVar = rho;

    break;
case FRONT:
    l1 = 1;
    l2 = 0;
    l3 = 0;
    // u1 = uz // u2  = ux // u3 == uy
    ux1 = 0.0;
    ux2 = 0.0;
    ux3 = 0.0;

    rho_I = pop[0] + pop[1] + pop[2] + pop[3] + pop[4] + pop[5] + pop[7] + pop[8] + pop[9] + pop[11] + pop[13] + pop[14] + pop[16] + pop[18];
    inv_rho_I = 1.0 / rho_I;
    pixx_I = inv_rho_I * (-(1.0/3.0)*pop[0] + (2.0/3.0)*pop[1] + (2.0/3.0)*pop[2] - (1.0/3.0)*pop[3] - (1.0/3.0)*pop[4] - (1.0/3.0)*pop[5] + (2.0/3.0)*pop[7] + (2.0/3.0)*pop[8] + (2.0/3.0)*pop[9] - (1.0/3.0)*pop[11] + (2.0/3.0)*pop[13] + (2.0/3.0)*pop[14] + (2.0/3.0)*pop[16] - (1.0/3.0)*pop[18]);
    pixy_I = inv_rho_I * (pop[7] + pop[8] - pop[13] - pop[14]);
    pixz_I = inv_rho_I * (pop[9] - pop[16]);
    piyy_I = inv_rho_I * (-(1.0/3.0)*pop[0] - (1.0/3.0)*pop[1] - (1.0/3.0)*pop[2] + (2.0/3.0)*pop[3] + (2.0/3.0)*pop[4] - (1.0/3.0)*pop[5] + (2.0/3.0)*pop[7] + (2.0/3.0)*pop[8] - (1.0/3.0)*pop[9] + (2.0/3.0)*pop[11] + (2.0/3.0)*pop[13] + (2.0/3.0)*pop[14] - (1.0/3.0)*pop[16] + (2.0/3.0)*pop[18]);
    piyz_I = inv_rho_I * (pop[11] - pop[18]);
    pizz_I = inv_rho_I * (-(1.0/3.0)*pop[0] - (1.0/3.0)*pop[1] - (1.0/3.0)*pop[2] - (1.0/3.0)*pop[3] - (1.0/3.0)*pop[4] + (2.0/3.0)*pop[5] - (1.0/3.0)*pop[7] - (1.0/3.0)*pop[8] + (2.0/3.0)*pop[9] + (2.0/3.0)*pop[11] - (1.0/3.0)*pop[13] - (1.0/3.0)*pop[14] + (2.0/3.0)*pop[16] + (2.0/3.0)*pop[18]);

    p_x1_x1_I = pizz_I;
    p_x2_x2_I = pixx_I;
    p_x3_x3_I = piyy_I;
    p_x1_x2_I = pixz_I;
    p_x1_x3_I = piyz_I;
    p_x2_x3_I = pixy_I;

    #include "momFace"


    ux_t30 = ux2;
    uy_t30 = ux3;
    uz_t30 = ux1;

    pixx_t45 = p_x2_x2;
    piyy_t45 = p_x3_x3;
    pizz_t45 = p_x1_x1;
    pixy_t90 = p_x2_x3;
    pixz_t90 = p_x1_x2;
    piyz_t90 = p_x1_x3;

    rhoVar = rho;

    break;

default:
        break;

rhoVar = rho;
}